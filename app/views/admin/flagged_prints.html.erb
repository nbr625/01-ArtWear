<table class="table table-hover" style="float: left;">
	<tr><strong>Flagged Prints</strong></tr>
	<% @prints.sort_by(&:flag_count).reverse.each do |print| %>	
	<tr>
		<% if print.flag_count > 0 %>
			<td><%= link_to image_tag print.image.url(:thumb) %></td>
			<td><%= link_to print.name, print_path(print) %></td>
			<td><%= print.flag_count %></td>
			<td><%= link_to print.user.username, user_path(print.user) %></td>
		<% end %>
	<% end %>
	</tr>
</table>
<div><%= link_to "Back to Admin Main", admin_main_path, class: "btn btn-default" %></div>