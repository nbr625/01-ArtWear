<h1>Admin#main</h1>
<p>Find me in app/views/admin/main.html.erb</p>

<table class="table table-hover" style="float: left;">
	<tr>Best Rated Prints</tr>
	<% @prints.sort_by(&:average_review).reverse.first(10).each do |print| %>	
	<tr>
		<td><%= link_to print.name, print_path(print) %>
		<td><%= print.average_review %></td>
		<td><%= link_to print.user.username, user_path(print.user) %></td>
	<% end %>
	</tr>
</table>

