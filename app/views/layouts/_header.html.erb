      <% if user_signed_in? %>
        <li class="navbar_li"><%= link_to "Profile", current_user, class: "navbar_element", style: "color: #188945;" %></li>
        <% if current_user.admin?%>
          <%= link_to "Admin Side", admin_main_path %>
        <% end %>
      <% else %>
        <li class="navbar_li"><%= link_to "Sign Up!", new_user_registration_path, class: "navbar_element", style: "color: #188945;" %></li>
        <li class="navbar_li"><%= link_to "Sign in", new_user_session_path, class: "navbar_element", style: "color: #188945;"%></li>
      <% end %>
     
    </ul>
  </div>
</div> 